{
  "name": "ngx-quill-upload",
  "version": "2.0.0",
  "description": "A module for images, videos and pdfs  to be uploaded to a server instead of being base64 encoded, in ngx-quill from toolbar editor.",
  "scripts": {
    "build": "ng-packagr -p package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semantika-it/ngx-quill-upload.git"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/package.schema.json",
    "lib": {
      "entryFile": "public_api.ts",
      "umdModuleIds": {
        "quill": "Quill"
      }
    },
    "dest": "dist"
  },
  "keywords": [
    "quill",
    "ngx-quill",
    "imageHandler",
    "videoHandler",
    "attachmentHandler"
  ],
  "author": "Zeno Mioso <zeno@semantika.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/semantika-it/ngx-quill-upload/issues"
  },
  "homepage": "https://github.com/semantika-it/ngx-quill-upload#readme",
  "devDependencies": {
    "@angular/cli": "10.2.1",
    "@angular/common": "10.0.11",
    "@angular/compiler": "10.0.11",
    "@angular/compiler-cli": "10.0.11",
    "@angular/core": "10.0.11",
    "ng-packagr": "10.1.0",
    "quill": "1.3.7",
    "typescript": "3.9.7"
  },
  "peerDependencies": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
